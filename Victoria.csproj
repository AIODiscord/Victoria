<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <NoWarn>1591, 1571</NoWarn>
        <LangVersion>latest</LangVersion>
        <TargetFramework>netstandard2.0</TargetFramework>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    </PropertyGroup>

    <PropertyGroup>
        <Version>2.3.4</Version>
        <Title>Victoria</Title>
        <Authors>Yucked</Authors>
        <Description>Lavalink wrapper written for Discord.NET.</Description>
        <Copyright>2018 Yucked. All rights reserved.</Copyright>
        <PackageReleaseNotes>Added LavaPlayer#Skip. Fixed Queue (It didn't need to be concurrentdictionary). Fixed objects updating via events that were being omitted from lavalink and this should fix Player#Position.
            Fixed a bug where TrackStuck and TrackException were mixed up. Calling LavaNode#LeaveAsync disposes of everything within the player.
        </PackageReleaseNotes>
        <PackageTags>Lavalink, Wrapper, Discord Music, Yucked</PackageTags>
        <PackageProjectUrl>https://github.com/Yucked/Victoria</PackageProjectUrl>
        <PackageIconUrl>https://i.imgur.com/F34NrfQ.png</PackageIconUrl>
    </PropertyGroup>

    <PropertyGroup Condition=" '$(Configuration)' == 'Debug' ">
        <DocumentationFile>bin\Debug\netstandard2.0\Victoria.xml</DocumentationFile>
    </PropertyGroup>

    <PropertyGroup Condition=" '$(Configuration)' == 'Release' ">
        <DocumentationFile>bin\Release\netstandard2.0\Victoria.xml</DocumentationFile>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="Discord.Net" Version="2.0.0-beta2-01001"/>
        <PackageReference Include="HyperEx" Version="1.0.5"/>
    </ItemGroup>

</Project>
